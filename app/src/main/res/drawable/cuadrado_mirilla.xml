<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="center">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent" />
            <size
                android:width="38dp"
                android:height="38dp" />
            <stroke
                android:width="2dp"
                android:color="#DE5B14" />
        </shape>
    </item>
    <item android:gravity="center">
        <shape android:shape="rectangle">
            <size
                android:width="25dp"
                android:height="25dp" />
            <solid android:color="@android:color/transparent" />
            <stroke
                android:width="2dp"
                android:color="@color/white" />
        </shape>
    </item>

    <item android:gravity="center">
        <shape android:shape="rectangle">
            <size
                android:width="29dp"
                android:height="29dp" />
            <solid android:color="@android:color/transparent" />
            <stroke
                android:width="2dp"
                android:color="#DE5B14" />
        </shape>
    </item>
</layer-list>